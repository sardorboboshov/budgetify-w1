{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "linebreak-style": "off",
    "comma-dangle": "off",
    "no-console": "off",
    "camelcase": "off",
    "object-curly-newline": "off",
    "no-undef": "off",
    "operator-linebreak": "off",
    "no-underscore-dangle": "off",
    "consistent-return": "off"
  }
}
